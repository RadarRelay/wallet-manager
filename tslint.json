{
  "extends": [
    "tslint-config-0xproject"
  ],
  "rules": {
    "no-console": false,
    "no-shadowed-variable": false,
    "trailing-comma": false,
    "align": false,
    "curly": [true, "ignore-same-line"],
    "no-namespace": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "class-name": false
  }
}
